{
    "compilerOptions": { /* Docs: https://www.typescriptlang.org/tsconfig */
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "../10min-server/node_modules/commons/bin",
      "declaration": true,
      "rootDir": "src",
      "strict": true,
      "noImplicitAny": false,
      "strictNullChecks": true,
      "checkJs": true,
      "allowJs": true,
      "moduleResolution": "node",
      "types": [
        "node"
      ],
      "lib": [
        "es6"
      ],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowUnreachableCode": true,
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "<node_internals>/**",
    ]
  }